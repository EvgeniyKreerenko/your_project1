@model IEnumerable<Restaurant.Models.RestItem>

@{
    ViewBag.Title = "Search";
}

<div class="container-fluid">
    <div class="row">
        <div class="col-sm-6 col-md-6 main text-center">

            <h3 class="page-header">Menu search</h3>

            <div id="searchresults">
                <h3>Menu items with @Model.First().Title</h3>
                <ul>
                    @foreach (var restItem in Model)
                    {
                        <li>@restItem.Title</li>
                    }
                </ul>
            </div>
        </div>
    </div>
</div>
